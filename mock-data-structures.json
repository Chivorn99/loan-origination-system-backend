    "testScenarios": [
      {
        "name": "Complete Loan Lifecycle",
        "steps": [
          "1. Create customer",
          "2. Create collateral item for customer",
          "3. Create loan using customer and collateral",
          "4. Make repayment for the loan",
          "5. Redeem the loan",
          "6. Verify all status changes"
        ]
      },
      {
        "name": "Customer Management Flow",
        "steps": [
          "1. Create multiple customers",
          "2. Update customer details",
          "3. Search customers by ID number",
          "4. Filter customers by status",
          "5. Soft delete a customer",
          "6. Verify deletion doesn't remove records"
        ]
      },
      {
        "name": "Reporting and Analytics",
        "steps": [
          "1. Create loans with different dates",
          "2. Create repayments across date ranges",
          "3. Test date range queries",
          "4. Test daily collection reports",
          "5. Test customer repayment summaries",
          "6. Verify pagination works correctly"
        ]
      }
    ],
    "dataGenerationRules": {
      "customer": {
        "requiredFields": ["fullName", "idNumber"],
        "validation": "idNumber must be unique",
        "statusDistribution": {
          "ACTIVE": 70,
          "INACTIVE": 20,
          "DELETED": 10
        }
      },
      "pawnItem": {
        "requiredFields": ["customerId", "itemType", "estimatedValue"],
        "statusDistribution": {
          "AVAILABLE": 40,
          "PAWNED": 40,
          "REDEEMED": 10,
          "FORFEITED": 10
        },
        "itemTypes": ["Gold Jewelry", "Electronics", "Vehicles", "Documents", "Other Valuables"]
      },
      "pawnLoan": {
        "requiredFields": ["customerId", "pawnItemId", "currencyId", "branchId", "loanAmount", "interestRate", "dueDate"],
        "amountRange": {
          "min": 100,
          "max": 10000
        },
        "interestRateRange": {
          "min": 3,
          "max": 10
        },
        "termDays": {
          "min": 30,
          "max": 180
        }
      },
      "pawnRepayment": {
        "validation": "paidAmount must equal sum of principalPaid, interestPaid, and penaltyPaid",
        "paymentDateLogic": "Should be on or after loan creation date, on or before current date"
      }
    }
